{
    "name": "cleanique-coders/route-macro",
    "description": "Built with Laravel Standalone Package Creator",
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\RouteMacro\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\RouteMacro\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/routing": "^5.5",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "~3.0",
        "codedungeon/phpunit-result-printer": "^0.4.4"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
